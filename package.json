  "homepage": "https://github.com/forivall/$NAME",
  "repository": {
    "type": "git",
    "url": "git@github.com:forivall/$NAME.git"
  },
  "bugs": {
    "url": "https://github.com/forivall/$NAME/issues"
  },
  "scripts": {
    "lint": "xo",
    "lint-staged": "lint-staged",
    "inspect": "jsinspect",
    "test": "ava",
    "posttest": "npm run lint && npm run inspect",
    "test-cov": "nyc ava",
    "posttest-cov": "npm run posttest",
    "test-ci": "npm run test-cov",
    "posttest-ci": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "dependencies": {},
  "bundledDependencies": [],
  "peerDependencies": {},
  "devDependencies": {
    "ava": "^0.18.2",
    "coveralls": "^2.11.14",
    "eslint-config-mxo": "^0.4.0",
    "jsinspect": "^0.10.1",
    "nyc": "^10.1.2",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": [
      "mxo"
    ],
    "overrides": [
      {
        "files": [
          "test/**",
          "test.js",
          "test-*.js",
          "**/__tests__/**",
          "**/*.test.js",
          "!**/fixtures/**",
          "!**/helpers/**"
        ],
        "extends": [
          "mxo/ava"
        ]
      }
    ]
  }
